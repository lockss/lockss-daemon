    <entry>
    <string>au_crawlrules</string>
    <list>
      <string>4,"^%s", base_url</string>
      <string>1,"^%sNCAW-Vol%s\.html$", base_url, volume_name</string>
      <!-- MF - had to add .* to this to get the main article pages, was only getting the print pages without it -->
      <string>1,"^%s(index\.php/)?(autumn|spring|winter|summer)%02d(index)?/.*", base_url, au_short_year</string> <!-- MF first change, adding index.php and removing _-->
      <string>1,"^https?://(%s|code\.jquery\.com)/[^?]*\.(bmp|css|dfont|eot|gif|ico|jpe?g|js|otf|png|svg|tif?f|ttc|ttf|woff.?)(\?.*)?$", url_host(base_url)</string>
      <string>1,"^https?://(www.)?19thc-artworldwide.org/.*\.pdf", url_host(base_url)</string> <!--temp hard code because there's possibly no www in link -->
    </list>
  </entry>
