  <!-- The volume_name parameter may now be a set, to handle AUs
       containing issues from more than one volume.  This could result
       in overcrawls if there are links from issues intended to be in
       the AU to issues not intended to be in the AU.  If that happens
       (it didn't with the first multi-volume AU we tried), it will be
       necessary for the crawl rules to match specific volume/issue
       pairs.  One way to do that is to make the set elements be
       vol:issue pairs and define two custom param functors that
       return the first & second elements of the pair.  Unfortunately
       that would be incompatible with all the existing AUs.  A hack
       to do it compatibly would be to make the functors work on
       non-pairs: the first would return the value (volume), the
       second would return something that allows the regexp to match
       any issue.  Simply returning .* probably won't work because it
       will be escaped to \.\* when substituted into the regexp, but
       something along these lines should be possible. -->

  <entry>
    <string>au_crawlrules</string>
    <list>
<!--      <string>1,"^https?://(.+\.cloudfront\.net|cdn\.mathjax\.org|.+\.jquery\.com|(fonts|ajax)\.(gstatic|googleapis)\.com)/."</string> -->
      <!-- exclude email links and improperly relative links -->
      <string>2,"^https://%s/articles/[0-9]+/files/.*@.*\.edu",url_host(base_url)</string>
      <string>2,"^https://%s/articles/[0-9]+/files/www\.",url_host(base_url)</string>
      <string>2,"^https://%s/articles/[0-9]+/files/.*\.(com|net|org|edu|gov)", url_host(base_url)</string>
      <!-- bad url link in Journal of Conservation and Museum Studies Volume 2012 -->
      <string>2,"https://jcms-journal\.com/articles/31/files/submission/proof/.r20"</string>
      <!-- bad url link in Journal of Technology Education Volume 1991; publisher said to ignore this -->
      <string>2,"https://jte-journal\.org/articles/376/files/submission/proof/376-1-903-5-10-20220801\.html"</string>
      <!--  take out jquery temporarily to see what it is that this needed -->
      <string>1,"^https?://(maxcdn\.bootstrapcdn\.com|cdnjs\.cloudflare\.com|.+\.cloudfront\.net|cdn\.mathjax\.org|(fonts|ajax|storage)\.(gstatic|googleapis)\.com)/."</string>
      <string>1,"^https?://%s%sapi/theme$", url_host(base_url), url_path(base_url)</string>
      <string>1,"^https?://%s/index.php/%s/issue/view/[0-9]+$", url_host(base_url2), journal_id</string>
      <string>1,"^https?://%s/index.php/%s/.*/css\?name=(stylesheet|font)$", url_host(base_url2), journal_id</string>
      <string>1,"^https?://%s/api/[0-9\.]+/spritesheet", url_host(base_url)</string>
      <string>1,"^https?://%s%slockss/year/%d/?$", url_host(base_url), url_path(base_url), year</string>
      <string>1,"^https?://%s%s.*\.(bmp|css|eot|gif|ico|js|jpe?g|png|tif?f|svg|ttf|woff.?)([?]((itok|v)=)?[^&amp;]+)?(/)?$", url_host(base_url2), url_path(base_url2)</string>
      <string>1,"^https?://%s%s.*\.(bmp|css|eot|gif|ico|js|jpe?g|png|tif?f|svg|ttf|woff.?)([?]((itok|v)=)?[^&amp;]+)?(/)?$", url_host(base_url), url_path(base_url)</string>
      <string>1,"^https?://journal-assets\.ubiquity\.press/.*\.(bmp|css|eot|gif|ico|js|jpe?g|png|tif?f|svg|ttf|woff.?)$"</string>
      <string>1,"^https?://%s%s_next/image\?url=.*\.(bmp|css|eot|gif|ico|js|jpe?g|png|tif?f|svg|ttf|woff.?)", url_host(base_url), url_path(base_url)</string>
      <string>1,"^https?://%s%sindex.php/%s/.*/page/page/css\?name=(stylesheet|font)", url_host(base_url), url_path(base_url2), journal_id</string>
      <string>1,"^https?://%s%s((en|es|fr)/)?articles(/abstract)?/[a-zA-Z0-9\.]+", url_host(base_url), url_path(base_url)</string>
      <string>1,"^https?://%s%sarticles/[^/?]+/thumbs/", url_host(base_url2), url_path(base_url2)</string>
      <string>1,"^https?://%s%s((en|es|fr)/)?[^/?]+/volume/(%s|0)/issue/", url_host(base_url), url_path(base_url), volume_name</string>
      <string>1,"^https?://%s%s[^/?]+(/volume/[^/?]+)?/issue/", url_host(base_url2), url_path(base_url2)</string>
      <string>1,"^https?://%s%sarticles/[^/?]+/files/[^?]+\.(xml|pdf|html)$",url_host(base_url), url_path(base_url)</string>
      <string>1,"^https?://%s/about/?",url_host(base_url)</string>
      <string>1,"^https?://%s/lockss/year/%d",url_host(base_url), year</string>
    </list>
  </entry>
