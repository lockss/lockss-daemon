      <entry>
        <string>Poll</string>
        <string>5</string>
      </entry>
      <entry>
        <string>Metadata</string>
        <string>3</string>
      </entry>
      <entry>
        <string>Substance</string>
        <string>1</string>
      </entry>
    </map>
  </entry>
  <entry>
    <string>au_start_url</string>
    <list>
      <string>"%slockss/year/%d", to_http(base_url), year</string>
      <string>"%slockss/year/%d", to_https(base_url), year</string>
    </list>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <list>
<!--      <string>1,"^https?://(.+\.cloudfront\.net|cdn\.mathjax\.org|.+\.jquery\.com|(fonts|ajax)\.(gstatic|googleapis)\.com)/."</string> -->
<!--  take out jquery temporarily to see what it is that this needed -->
      <string>1,"^https?://(maxcdn\.bootstrapcdn\.com|cdnjs\.cloudflare\.com|.+\.cloudfront\.net|cdn\.mathjax\.org|(fonts|ajax)\.(gstatic|googleapis)\.com)/."</string>
      <string>1,"^https?://%s%slockss/year/%d/?$", url_host(base_url), url_path(base_url), year</string>
      <string>1,"^https?://%s%s.*\.(bmp|css|eot|gif|ico|js|jpe?g|png|tif?f|svg|ttf|woff.?)([?]((itok|v)=)?[^&amp;]+)?(/)?$", url_host(base_url), url_path(base_url)</string>
      <string>1,"^https?://%s%sarticles(/abstract)?/[^/?]+/[^/?]+/?(galley/[^/?]+/download)?/?$", url_host(base_url), url_path(base_url)</string>
      <string>1,"^https?://%s%sarticles/[^/?]+/thumbs/", url_host(base_url), url_path(base_url)</string>
      <string>1,"^https?://%s%s[^/?]+/volume/[^/?]+/issue/[^/]+/$", url_host(base_url), url_path(base_url)</string>
    </list>
  </entry>
  <entry>
    <string>au_substance_url_pattern</string>
      <list>
        <string>"^https?://%s%sarticles/[^/?]+/[^/?]+/galley/[^/?]+/download/?$", url_host(base_url), url_path(base_url)</string>
      </list>
  </entry>
  <entry>
    <string>au_redirect_to_login_url_pattern</string>
    <string>"%slogin", base_url</string>
  </entry>
  <entry>
    <string>plugin_name</string>
    <string>Ubiquity Partner Network Journals Plugin</string>
  </entry>
  <entry>
    <string>au_name</string>
    <string>"Ubiquity Partner Network Journals Plugin, Base URL %s, Year %d", base_url, year</string>
  </entry>
  <entry>
    <string>plugin_config_props</string>
    <list>
      <org.lockss.daemon.ConfigParamDescr>
        <key>year</key>
        <displayName>Year</displayName>
        <description>Four digit year (e.g., 2004)</description>
        <type>4</type>
        <size>4</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>base_url</key>
        <displayName>Base URL</displayName>
        <description>Usually of the form http://&lt;journal-name&gt;.com/</description>
        <type>3</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
    </list>
  </entry>
