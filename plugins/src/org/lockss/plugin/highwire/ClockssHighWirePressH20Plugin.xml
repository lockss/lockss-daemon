  <entry>
    <string>required_daemon_version</string>
    <string>1.53.0</string>
  </entry>
  <entry>
    <string>plugin_config_props</string>
    <list>
      <org.lockss.daemon.ConfigParamDescr>
        <key>base_url</key>
        <displayName>Base URL</displayName>
        <description>Usually of the form http://&lt;journal-name&gt;.com/</description>
        <type>3</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>volume_name</key>
        <displayName>Volume Name</displayName>
        <type>1</type>
        <size>20</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
    </list>
  </entry>
  <entry>
    <string>au_name</string>
    <string>&quot;HighWire Press Plugin (H20 for CLOCKSS), Base URL %s, Volume %s&quot;, base_url, volume_name</string>
  </entry>
  <entry>
    <string>au_start_url</string>
    <string>&quot;%sclockss-manifest/vol_%s_manifest.dtl&quot;, base_url, volume_name</string>
  </entry>
  <entry>
    <string>au_feature_urls</string>
    <map>
      <entry>
        <string>au_title</string>
        <string>&quot;%s&quot;, base_url</string>
      </entry>
      <entry>
        <string>au_volume</string>
        <string>&quot;%sclockss-manifest/vol_%s_manifest.dtl&quot;, base_url, volume_name</string>
      </entry>
      <entry>
        <string>au_issue</string>
        <string>&quot;%scontent/%s/%s.toc&quot;, base_url, volume_name, issue</string>
      </entry>
      <entry>
        <string>au_article</string>
        <map>
          <entry>
            <string>pdf+html;*</string>
            <string>&quot;%scontent/%s/%s/%s.full.pdf+html&quot;, base_url, volume_name, issue, article</string>
          </entry>
          <entry>
            <string>pdf</string>
            <string>&quot;%scontent/%s/%s/%s.full.pdf&quot;, base_url, volume_name, issue, article</string>
          </entry>
          <entry>
            <string>html</string>
            <string>&quot;%scontent/%s/%s/%s.full&quot;, base_url, volume_name, issue, article</string>
          </entry>
        </map>
      </entry>
    </map>
  </entry>  
  <entry>
    <string>au_permission_checker_factory</string>
    <string>org.lockss.plugin.highwire.HighWirePermissionCheckerFactory</string>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <list>
      <string>2,&quot;^%s.*\.cover\.gif$&quot;, base_url</string>
      <string>1,&quot;^https?://%s/[^?]*\.(bmp|css|gif|ico|jpe?g|js|png|tif?f)$&quot;, base_url_host</string>
      <string>4,&quot;^%s&quot;, base_url</string>
      <string>1,&quot;^%sclockss-manifest/vol_%s_manifest\.dtl$&quot;, base_url, volume_name</string>
      <string>2,&quot;^%s.*\?tab=(metrics|related|responses)&quot;, base_url</string>
      <string>2,&quot;^%s(content|lookup/volpage)/%s/.*(cited-by|patientinform-links|related-urls)=yes&quot;, base_url, volume_name</string>
      <string>2,&quot;^%scontent/%s/.*/href/?$&quot;, base_url, volume_name</string>
      <string>2,&quot;^%slookup/volpage/%s(/[^/]+)?$&quot;, base_url, volume_name</string>
      <!-- RU4166: exclude oxford supplement author-index pages, e.g.: http://humrep.oxfordjournals.org/content/26/suppl_1.author-index -->
      <string>2,&quot;^%scontent/%s/suppl_[0-9]*\.author-index$&quot;, base_url, volume_name</string>
      <string>1,&quot;^%s(content|lookup/volpage)/%s(/|\.)&quot;, base_url, volume_name</string>
      <string>1,&quot;^%s(cgi/content/[^/]+/%s|content/vol%s)/&quot;, base_url, volume_name, volume_name</string>
      <string>1,&quot;^%s[^?]*DC(Supplemental|[0-9])&quot;, base_url</string>
      <string>2,&quot;^%shighwire/filestream/powerpoint/&quot;, base_url</string>
      <string>1,&quot;^%shighwire/filestream/&quot;, base_url</string>
    </list>
  </entry>
