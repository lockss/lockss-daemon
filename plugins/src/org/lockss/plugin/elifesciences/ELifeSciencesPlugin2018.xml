    <string>org.lockss.plugin.elifesciences.ELife2018HtmlHashFilterFactory</string>
  </entry>
  <entry>
    <string>text/html_crawl_filter_factory</string>
    <string>org.lockss.plugin.elifesciences.ELife2018HtmlCrawlFilterFactory</string>
  </entry>
    <entry>
    <string>plugin_article_iterator_factory</string>
    <string>org.lockss.plugin.elifesciences.ELife2018ArticleIteratorFactory</string>
  </entry>
  <entry>
    <string>plugin_article_metadata_extractor_factory</string>
    <string>org.lockss.plugin.elifesciences.ELife2018ArticleIteratorFactory</string>
  </entry>
  <entry>
    <string>text/html_metadata_extractor_factory_map</string>
    <map>
      <entry>
        <string>*</string>
        <string>org.lockss.plugin.elifesciences.ELifeHtmlMetadataExtractorFactory</string>
      </entry>
    </map>
  </entry>
  <entry>
    <string>au_def_pause_time</string>
    <long>18000</long>
  </entry>
  <entry>
    <string>au_def_new_content_crawl</string>
    <long>1209600000</long>
  </entry>
</map>
