      <string>1,"^https?://%s/%s/issues/%d/%s/", url_host(base_url), journal_id, year, issue</string>
      <string>1,"^https?://%s/cgi-bin/(sendcif|sendsup|sendsupfiles)\?", url_host(scripts_url)</string>
    </list>
  </entry>
