	    <from>12:00</from>
	    <to>24:00</to>
	    <timeZoneId>America/Los_Angeles</timeZoneId>
	  </org.lockss.daemon.CrawlWindows-Daily>
	  <org.lockss.plugin.RateLimiterInfo>
	    <rate>1/2s</rate>
	  </org.lockss.plugin.RateLimiterInfo>
	</entry>
    <entry>
	  <org.lockss.daemon.CrawlWindows-Daily>
	    <from>24:00</from>
	    <to>12:00</to>
	    <timeZoneId>America/Los_Angeles</timeZoneId>
	  </org.lockss.daemon.CrawlWindows-Daily>
	  <org.lockss.plugin.RateLimiterInfo>
	    <rate>1/6s</rate>
