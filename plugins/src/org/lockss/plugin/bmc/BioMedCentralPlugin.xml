  <entry>
    <string>plugin_name</string>
    <string>BioMed Central Plugin (BMC Journals and Chemistry Central)</string>
  </entry>
  <entry>
    <string>required_daemon_version</string>
    <string>1.68.4</string> <!-- Daemon version at time of update -->
  </entry>
