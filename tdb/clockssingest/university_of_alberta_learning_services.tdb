        implicit < status ; status2 ; year ; name ; param[year] ; volume ; hidden[proxy] >
        
        #manifest pages don't have links, for example https://journals.library.ualberta.ca/eblip/index.php/EBLIP/gateway/lockss?year=2018

        au < manifest ; exists ; 2006 ; Evidence Based Library and Information Practice Volume 1 ; 2006 ; 1 ; >
        au < manifest ; exists ; 2007 ; Evidence Based Library and Information Practice Volume 2 ; 2007 ; 2 ; >
        au < manifest ; exists ; 2008 ; Evidence Based Library and Information Practice Volume 3 ; 2008 ; 3 ; >
        au < manifest ; exists ; 2009 ; Evidence Based Library and Information Practice Volume 4 ; 2009 ; 4 ; >
