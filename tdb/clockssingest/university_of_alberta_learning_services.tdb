{

  publisher <
    name = University of Alberta Learning Services ;
    info[contract] = 2011 ;
    info[back] = 2006 ;
    info[tester] = 8 ;
    info[poller] = 8
  >

    comment[eblip] = Only two titles are participating in clockss from this publisher.
    # "Evidence Based Library and Information Practice" 
    # "Journal of Pharmacy and Pharmaceutical Science" 
    attr[start_stem] = index.php/  #for OJS3

    {
    
      title <
        name = Journal of Pharmacy and Pharmaceutical Science ;
        eissn = 1482-1826
      >
      
        param[base_url] = https://journals.library.ualberta.ca/jpps/
        implicit < status ; status2 ; year ; name ; param[year] ; volume ; hidden[proxy] >
        param[journal_id] = JPPS

        {
        plugin = org.lockss.plugin.ojs3.ClockssOjs3Plugin 

        au < frozen ; exists ; 2018 ; Journal of Pharmacy and Pharmaceutical Science Volume 21 ; 2018 ; 21 ; >
        au < frozen ; exists ; 2019 ; Journal of Pharmacy and Pharmaceutical Science Volume 22 ; 2019 ; 22 ; >
        au < crawling ; exists ; 2020 ; Journal of Pharmacy and Pharmaceutical Science Volume 23 ; 2020 ; 22 ; >
        au < crawling ; exists ; 2021 ; Journal of Pharmacy and Pharmaceutical Science Volume 24 ; 2021 ; 23 ; >
        }
        {
        plugin = org.lockss.plugin.ojs2.ClockssOJS2Plugin 

        au < finished ; finished ; 2007 ; Journal of Pharmacy and Pharmaceutical Science Volume 10 ; 2007 ; 10 ; reingest1.clockss.org:8082 >
        au < finished ; finished ; 2008 ; Journal of Pharmacy and Pharmaceutical Science Volume 11 ; 2008 ; 11 ; reingest2.clockss.org:8085 >
        au < finished ; finished ; 2009 ; Journal of Pharmacy and Pharmaceutical Science Volume 12 ; 2009 ; 12 ; reingest3.clockss.org:8083 >
        au < finished ; finished ; 2010 ; Journal of Pharmacy and Pharmaceutical Science Volume 13 ; 2010 ; 13 ; reingest4.clockss.org:8082 >
        au < finished ; finished ; 2011 ; Journal of Pharmacy and Pharmaceutical Science Volume 14 ; 2011 ; 14 ; reingest5.clockss.org:8082 >
