    {
    plugin = org.lockss.plugin.clockss.casalini.ClockssCasaliniLibriBooksPlugin

    au < readySource ; crawling ; 2020 ; Casalini Books Source Content 2020 ; 2020 >
    au < readySource ; crawling ; 2021 ; Casalini Books Source Content 2021 ; 2021 >
    }
    {
    plugin = needs.SourcePlugin

    au < expected ; exists ; 2022 ; Casalini Books Source Content 2022 ; 2022_01 >
    }
  }

}
