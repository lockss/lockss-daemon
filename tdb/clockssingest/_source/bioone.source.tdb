{
  publisher <
    name = American Entomological Society ;
    info[contract] = 2014 ;
    info[tester] = S ;
    info[platform] = Source
  >

    provider = American Entomological Society
    hidden[proxy] = DIRECT
    implicit < status ; status2 ; year ; name ; param[directory] >
    plugin = org.lockss.plugin.clockss.bioone.ClockssBioOneSourcePlugin

  {

    title <
       name = American Entomological Society Source Content
    >
    {
      param[base_url] = http://clockss-ingest.lockss.org/sourcefiles/aes-released/

      au < readySource ; frozen ; 2022 ; American Entomological Society Source Content 2022_01 ; 2022_01 >
      #No more AUs. Moved to https.

    }
    {
      param[base_url] = https://clockss-ingest.lockss.org/sourcefiles/aes-released/

      au < expected ; exists ; 2023 ; American Entomological Society Source Content 2023_01 ; 2023_01 >

    }

  }
}
