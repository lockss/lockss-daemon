    #No more AUs. Moving to https.
    }
    {
    param[base_url] = https://clockss-ingest.lockss.org/sourcefiles/iosbooks-released/

    au < readySource ; crawling ; 2023 ; IOS Press Books Source Content 2023 ; 2023 >
    }
  }

  {

    title <
       name = IOS Press Source Content
    >

