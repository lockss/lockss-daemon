{
  publisher <
    name = Mineralogical Society of America ;
    info[contract] = 2013 ;
    info[tester] = S ;
    info[platform] = Source/WARC
  >

    ### No need to re-collect or re-spin the Warc unless there is a change ###

    plugin = org.lockss.plugin.clockss.warc.ClockssWarcPlugin
    hidden[proxy] = DIRECT
    implicit < status ; status2 ; year ; name ; param[year] >
  {
    title < 
      name = American Mineralogist ;
      issn = 0003-004X ;
      eissn = 1945-3027
    >

    # This content was collected using wget as a set of WARC files
    # WARC content collected from http://www.minsocam.org/MSA/AmMin/TOC/
    # Free Access to the Abstracts & Articles in the American Mineralogist from 1916 to 1999.
    # See RT: https://support.lockss.org/Ticket/Display.html?id=8104

    param[base_url] = http://clockss-ingest.lockss.org/warcfiles/american-mineralogist-released/

    au < zapped ; finished ; 2016 ; American Mineralogist WARC Content 2016 ; 2016 >
    #done
  }

  {
    title < 
      name = Reviews in Mineralogy and Geochemistry ;
      issn = 1529-6466

    >
    # This content was collected using wget and provided for preservation as a
    # set of WARC files
    # WARC content collected from http://www.minsocam.org/MSA/RIM/
    # See RT: https://support.lockss.org/Ticket/Display.html?id=8104
    # Note from site: Full-text available from the year 2000 to current. Citations only from 1980 to 1999.
    # the Warc contains more than the content from 1980 to 1999, however, the 1980 to 1999 content is stabile

    param[base_url] = http://clockss-ingest.lockss.org/warcfiles/reviews-in-mineralogy-and-geochemistry-released/

    au < zapped ; finished ; 2016 ; Reviews in Mineralogy and Geochemistry WARC Content 2016 ; 2016 >
    au < zapped ; finished ; 2018 ; Reviews in Mineralogy and Geochemistry WARC Content 2018 ; 2018 >
    #done
  }

}
