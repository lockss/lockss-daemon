{

  publisher <
    name = American College of Cardiology ;
    info[platform] = BLOCKED GLN only. Atypon. Four titles (8 on website). GLN only. Need manifest pages
  >
  
  # publisher moving H10 -> Drupal -> Atypon
  # this tdb file has only 2019 and 2020. All back content processed under HW.

  plugin = org.lockss.plugin.atypon.acc.ACCJournalsPlugin
  implicit < status ; year ; name ; param[volume_name] >
  param[base_url] = https://www.jacc.org/

  {
    title <
      name = JACC: Basic To Translational Science ;
      eissn = 2452-302X
    >

    param[journal_id] = basic-translational

    au < exists ; 2019 ; JACC: Basic To Translational Science Volume 4 ; 4 >
    au < exists ; 2020 ; JACC: Basic To Translational Science Volume 5 ; 5 >
    au < testing ; 2021 ; JACC: Basic To Translational Science Volume 6 ; 6 >

  }

  {
    title <
      name = JACC: Cardiovascular Imaging ;
      issn = 1936-878X ;
      eissn = 1876-7591
    >

    param[journal_id] = imaging

    au < exists ; 2019 ; JACC: Cardiovascular Imaging Volume 12 ; 12 >
    au < exists ; 2020 ; JACC: Cardiovascular Imaging Volume 13 ; 13 >
    au < testing ; 2021 ; JACC: Cardiovascular Imaging Volume 14 ; 14 >


  }

  {
    title <
      name = JACC: Cardiovascular Interventions ;
      issn = 1936-8798 ;
      eissn = 1876-7605
    >

    param[journal_id] = interventions

    au < exists ; 2019 ; JACC: Cardiovascular Interventions Volume 12 ; 12 >
    au < exists ; 2020 ; JACC: Cardiovascular Interventions Volume 13 ; 13 >
    au < testing ; 2021 ; JACC: Cardiovascular Interventions Volume 14 ; 14 >

  }

  {

    title <
      name = Journal of the American College of Cardiology ;
      issn = 0735-1097 ;
      eissn = 1558-3597
    >

    param[journal_id] = jacc

    au < exists ; 2019 ; Journal of the American College of Cardiology Volume 73 ; 73 >
    au < exists ; 2019 ; Journal of the American College of Cardiology Volume 74 ; 74 >
    au < exists ; 2020 ; Journal of the American College of Cardiology Volume 75 ; 75 >
    au < exists ; 2020 ; Journal of the American College of Cardiology Volume 76 ; 76 >
    au < exists ; 2021 ; Journal of the American College of Cardiology Volume 77 ; 77 >
    au < testing ; 2021 ; Journal of the American College of Cardiology Volume 78 ; 78 >

  }
}
