{

  publisher <
    name = Manchester University Press
  >
  
    plugin = org.lockss.plugin.ojs2.OJS2Plugin
    implicit < status ; year ; param[year] ; name ; volume >

  {
    title <
      name = Ethnicity and Race in a Changing World ;
      eissn = 1758-8685 ;
      issnl = 1758-8685
    >
    {
      param[base_url] = https://ercw.openlibrary.manchester.ac.uk/
      param[journal_id] = ercw
      #The journal was cancelled after Volume 5, 2014
    
      au < released ; 2009 ; 2009 ; Ethnicity and Race in a Changing World Volume 1 ; 1 >
      au < released ; 2010 ; 2010 ; Ethnicity and Race in a Changing World Volume 2 ; 2 >
      au < released ; 2011 ; 2011 ; Ethnicity and Race in a Changing World Volume 3 ; 3 >
      au < released ; 2012 ; 2012 ; Ethnicity and Race in a Changing World Volume 4 ; 4 >
      au < released ; 2013 ; 2013 ; Ethnicity and Race in a Changing World Volume 5 ; 5 >
      au < released ; 2014 ; 2014 ; Ethnicity and Race in a Changing World Volume 6 ; 6 >
    }
    {
      param[base_url] = http://ercw.openlibrary.manchester.ac.uk/
      param[journal_id] = ercw
    
      au < superseded ; 2009 ; 2009 ; Ethnicity and Race in a Changing World Volume 1 [superseded] ; 1 >
      au < superseded ; 2010 ; 2010 ; Ethnicity and Race in a Changing World Volume 2 [superseded] ; 2 >
      au < superseded ; 2011 ; 2011 ; Ethnicity and Race in a Changing World Volume 3 [superseded] ; 3 >
      au < superseded ; 2012 ; 2012 ; Ethnicity and Race in a Changing World Volume 4 [superseded] ; 4 >
      au < superseded ; 2013 ; 2013 ; Ethnicity and Race in a Changing World Volume 5 [superseded] ; 5 >
      au < superseded ; 2014 ; 2014 ; Ethnicity and Race in a Changing World Volume 6 [superseded] ; 6 >
    }
  }
  
  {
    title <
      name = James Baldwin Journal ;
      eissn = 2056-9211 ;
      issnl = 2056-9211
    >
    {
      param[base_url] = https://jbr.openlibrary.manchester.ac.uk/
      param[journal_id] = jbr
      #move to PubFactory, https://www.manchesteropenhive.com/view/journals/jbr/jbr-overview.xml

      au < down ; 2015 ; 2015 ; James Baldwin Journal Volume 1 ; 1 >
      au < down ; 2016 ; 2016 ; James Baldwin Journal Volume 2 ; 2 >
      au < down ; 2017 ; 2017 ; James Baldwin Journal Volume 3 ; 3 >

    }
    {
      param[base_url] = http://jbr.openlibrary.manchester.ac.uk/
      param[journal_id] = jbr
    
      au < superseded ; 2015 ; 2015 ; James Baldwin Journal Volume 1 [superseded] ; 1 >
    }
  }

  {
    title <
      name = Manchester Medical Journal ;
      eissn = 2059-7126 ;
      issnl = 2059-7126
    >
    
      param[base_url] = https://mmj.openlibrary.manchester.ac.uk/
      param[journal_id] = mmj
    
      au < down ; 2016 ; 2016 ; Manchester Medical Journal Volume 1 ; 1 >
      au < down ; 2017 ; 2017 ; Manchester Medical Journal Volume 2 (2017) ; 2 >
      au < down ; 2018 ; 2018 ; Manchester Medical Journal Volume 2 (2018) ; 2 >
      # no 2019
      au < down ; 2020 ; 2020 ; Manchester Medical Journal Volume 3 ; 3 >
      # stopped publishing

  }

}