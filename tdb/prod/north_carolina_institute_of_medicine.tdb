  {

    title <
      name = North Carolina Medical Journal ;
      issn = 0029-2559 ;
      issnl = 0029-2559
    >

    {
    plugin = org.lockss.plugin.highwire.HighWirePressH20Plugin
    param[base_url] = http://www.ncmedicaljournal.com/
