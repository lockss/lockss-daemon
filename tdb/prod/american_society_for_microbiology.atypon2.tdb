{

  publisher <
    name = American Society for Microbiology ;
    info[platform] = Atypon
  >
  
  #Additional journals for this publisher/plugin are in an "atypon" tdb file.
  
  {
    plugin = org.lockss.plugin.atypon.asm.AsmPlugin
    param[base_url] = https://journals.asm.org/
    implicit < status ; year ; name ; param[volume_name] >
  
    {
      title <
        name = EcoSal Plus ;
        eissn = 2324-6200
      >
      
      param[journal_id] = ecosalplus
      
      au < doNotProcess ; 2004-2005 ; EcoSal Plus Volume 1 ; 1 >
      au < doNotProcess ; 2006-2007 ; EcoSal Plus Volume 2 ; 2 >
      au < doNotProcess ; 2008-2009 ; EcoSal Plus Volume 3 ; 3 >
      au < doNotProcess ; 2010-2011 ; EcoSal Plus Volume 4 ; 4 >
      au < doNotProcess ; 2012-2013 ; EcoSal Plus Volume 5 ; 5 >
      au < doNotProcess ; 2014-2015 ; EcoSal Plus Volume 6 ; 6 >
      au < doNotProcess ; 2016-2017 ; EcoSal Plus Volume 7 ; 7 >
      au < ready ; 2018-2019 ; EcoSal Plus Volume 8 ; 8 >
      au < manifest ; 2020-2021 ; EcoSal Plus Volume 9 ; 9 >

    }
  
    {
      title <
        name = Microbiology Spectrum ;
        eissn = 2165-0497
      >
      
      param[journal_id] = spectrum
      
      au < doNotProcess ; 2013 ; Microbiology Spectrum 2013 ; 1 >
      au < doNotProcess ; 2014 ; Microbiology Spectrum 2014 ; 2 >
      au < doNotProcess ; 2015 ; Microbiology Spectrum 2015 ; 3 >
      au < doNotProcess ; 2016 ; Microbiology Spectrum 2016 ; 4 >
      au < doNotProcess ; 2017 ; Microbiology Spectrum 2017 ; 5 >
      au < doNotProcess ; 2018 ; Microbiology Spectrum 2018 ; 6 >
      au < ready ; 2019 ; Microbiology Spectrum 2019 ; 7 >
      au < manifest ; 2020 ; Microbiology Spectrum 2020 ; 8 >
      au < exists ; 2021 ; Microbiology Spectrum 2021 ; 9 >

    }

  }

}
