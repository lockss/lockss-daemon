      au < released ; 2013 ; 3 ; Journal of Nursing and Health Volume 3 ; 2013 ; >
      au < expected ; 2014 ; 4 ; Journal of Nursing and Health Volume 4 ; 2014 ; >

  }

  {

      title <
        name = Revista Ciência da Madeira ;
        eissn = 2177-6830
      >

      comment[publisher] = Universidade Federal de Pelotas
